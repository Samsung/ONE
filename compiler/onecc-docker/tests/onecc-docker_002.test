# Check if docker image is created

filename_ext="$(basename -- $0)"
filename="${filename_ext%.*}"

trap_err_onexit()
{
  echo "${filename_ext} FAILED"
  exit 255
}

trap trap_err_onexit ERR

# run test
onecc-docker -v
RESULT=$(docker images -q onecc 2>&1)

if [[ -z "$RESULT" ]]; then
  trap_err_onexit
fi

echo "${filename_ext} SUCCESS"
