# run a workflow where one-import-tf -> one-optimize -> one-pack

filename_ext="$(basename -- $0)"
filename="${filename_ext%.*}"

trap_err_onexit()
{
  echo "${filename_ext} FAILED"
  exit 255
}

err_msg="onecc-docker: error: 401 Client Error: Unauthorized for url: https://api.github.com/repos/Samsung/ONE/releases/latest"

RESULT=$(onecc-docker -t err 2>&1)


if [[ ! ${err_msg} == ${RESULT} ]]; then
  trap_err_onexit
fi

echo "${filename_ext} SUCCESS"
