filename_ext="$(basename -- $0)"
filename="${filename_ext%.*}"

trap_err_onexit()
{
  echo "${filename_ext} FAILED"
  exit 255
}

trap trap_err_onexit ERR

inputfile="./test_onnx_model.onnx"
outputfile="./test_onnx_model.circle"
intermtflite="./test_onnx_model.tflite"
intermtf="./test_onnx_model.savedmodel"

rm -rf ${intermtf}
rm -rf ${intermtflite}
rm -rf ${outputfile}
rm -rf ${outputfile}.log

# run test
one-import-onnx \
--save_intermediate \
--input_path ${inputfile} \
--output_path ${outputfile} > ${outputfile}.log 2>&1

if [[ ! -s "${outputfile}" ]]; then
  trap_err_onexit
fi
if [[ ! -s "${intermtflite}" ]]; then
  trap_err_onexit
fi

echo "${filename_ext} SUCCESS"
