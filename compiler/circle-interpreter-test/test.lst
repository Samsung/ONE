#
# Format:
#   testcmd(TESTNAME {POS|NEG} COMMAND TO RUN ...)
#
#   ex.
#   testcmd(foo_000 POS echo foo)
#   testcmd(foo_001 NEG false foo)
#
# NOTE: Test can use enviornment variable `WORK_DIR` for model and data
#       `WORK_DIR` is set as path to `common-artifacts` directory
#

# Print help message
testcmd(POS_001 POS -h)

# Invoke circle-interpreter with proper arguments
testcmd(POS_002 POS ${WORK_DIR}/Conv2D_000.circle ${WORK_DIR}/Conv2D_000.circle.input /tmp/Conv2D_000.output)

# Invalid argument-1 : invoke circle-interpreter with wrong path to circle model
testcmd(NEG_001 NEG /invalid/path/to/circle/model ${WORK_DIR}/Conv2D_000.circle.input /tmp/Conv2D_000.output)

# Invalid argument-2 : invoke circle-interpreter with wrong path to input data
testcmd(NEG_002 NEG ${WORK_DIR}/Conv2D_000.circle /invalid/path/to/input/data /tmp/Conv2D_000.output)

# Invalid argument-3 : invoke circle-interpreter without passing required arguments
testcmd(NEG_003 NEG)
